{
	"compilerOptions": {
		"module": "ES2020",
		"target": "ES2020",
		"strict": true,
		"noImplicitOverride": true,
		"declaration": true,
		"strictNullChecks": false,
		"stripInternal": true,
		"outDir": "build",
		"esModuleInterop": true,
		"noImplicitAny": false,
		"paths": {
			"three": [
				"./typesDev/index.d.ts"
			]
		}
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"src/types"
	]
}