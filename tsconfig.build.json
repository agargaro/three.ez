{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"declaration": true,
		"sourceMap": true,
		"declarationMap": true,
		"noImplicitOverride": true,
		"strictNullChecks": false,
		"stripInternal": true,
		"outDir": "build",
		"esModuleInterop": true,
		"noImplicitAny": false,
	},
	"include": [
		"src/**/*.ts"
	]
}